<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Price</th>
    <th>Opncampaigns</th>
    <th>Opncustomerspercampaign</th>
    <th>Opcustomfield</th>
    <th>Opbackup</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.name %></td>
    <td><%= profile.description %></td>
    <td><%= profile.price %></td>
    <td><%= profile.opncampaigns %></td>
    <td><%= profile.opncustomerspercampaign %></td>
    <td><%= profile.opcustomfield %></td>
    <td><%= profile.opbackup %></td>
    <td><%= link_to 'Show', profile %></td>
    <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
    <td><%= link_to 'Destroy', profile, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile', new_profile_path %>
