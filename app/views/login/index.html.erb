<h1>QuickCall - Autenticazione</h1>


<%= form_tag({:controller => "login", :action => "verify"}, :method => "post", :class => "loginform") do %>
    
    <%= label_tag(:username, "Username") %>   
    <%= text_field_tag(:username) %>
 
    <%= label_tag(:password, "Password") %>
    <%= password_field_tag(:password) %>
  

    <%= submit_tag("Entra") %>
<% end %>
